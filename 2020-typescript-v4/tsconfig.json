{
	"include": ["./src/**/*"],
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"lib": ["dom", "es2017"],
		// "allowJs": true,
		// "checkJs": true,
		"jsx": "react",
		"sourceMap": true,
		"strict": true,
		"esModuleInterop": true,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"noEmit": true,
		"skipLibCheck": true
	}
}
